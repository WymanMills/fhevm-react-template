{
  "name": "confidential-property-valuation",
  "version": "1.0.0",
  "type": "module",
  "description": "Privacy-Preserving Real Estate Assessment Platform",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:hardhat": "hardhat test",
    "test:coverage": "vitest run --coverage",
    "test:hardhat:coverage": "hardhat coverage",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "lint:sol": "solhint 'contracts/**/*.sol'",
    "lint:sol:fix": "solhint 'contracts/**/*.sol' --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\"",
    "format:all": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,sol}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,css,sol}\"",
    "type-check": "tsc --noEmit",
    "typechain": "hardhat typechain",
    "deploy": "npm run build && gh-pages -d dist",
    "compile": "hardhat compile",
    "security:audit": "npm audit --audit-level=moderate",
    "security:check": "npx better-npm-audit audit --level moderate",
    "analyze": "npm run build -- --mode analyze",
    "prepare": "husky",
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-select": "^2.1.1",
    "@radix-ui/react-tabs": "^1.1.0",
    "@radix-ui/react-toast": "^1.2.1",
    "@rainbow-me/rainbowkit": "^2.1.6",
    "@tanstack/react-query": "^5.56.2",
    "@wagmi/core": "^2.13.5",
    "fhevm-sdk": "file:../../packages/fhevm-sdk",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "viem": "^2.21.4",
    "wagmi": "^2.12.12"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.8",
    "@nomicfoundation/hardhat-ethers": "^3.0.8",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.12",
    "@nomicfoundation/hardhat-toolbox": "^5.0.0",
    "@nomicfoundation/hardhat-verify": "^2.0.11",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@typechain/ethers-v6": "^0.5.1",
    "@typechain/hardhat": "^9.1.0",
    "@types/chai": "^4.3.20",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.5.5",
    "@types/react": "^18.3.9",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "@typescript-eslint/parser": "^8.8.0",
    "@vitejs/plugin-react": "^4.3.2",
    "@vitest/coverage-v8": "^2.1.2",
    "autoprefixer": "^10.4.20",
    "chai": "^4.5.0",
    "esbuild": "^0.24.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.12",
    "ethers": "^6.13.4",
    "gh-pages": "^6.1.1",
    "globals": "^15.11.0",
    "hardhat": "^2.22.18",
    "hardhat-deploy": "^0.12.4",
    "hardhat-gas-reporter": "^1.0.10",
    "husky": "^9.1.7",
    "jsdom": "^25.0.1",
    "lint-staged": "^15.5.2",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "prettier-plugin-solidity": "^1.4.3",
    "solhint": "^5.0.3",
    "solidity-coverage": "^0.8.16",
    "tailwindcss": "^3.4.13",
    "ts-node": "^10.9.2",
    "typechain": "^8.3.2",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.8.0",
    "vite": "^5.4.8",
    "vitest": "^2.1.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.sol": [
      "solhint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  }
}
